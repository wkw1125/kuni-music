<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kuni"
    android:versionCode="5"
    android:versionName="1.1">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.KuniActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.SettingsActivity"
            android:label="@string/title_activity_settings"></activity>
        <activity
            android:name=".activity.MyActionBarActivity"
            android:label="@string/title_activity_my_action_bar"></activity>
        <activity
            android:name=".activity.FavorActivity"
            android:label="@string/title_activity_favor"></activity>

        <service android:name="com.kwws.view.audioplayer.PlayService"></service>

        <activity
            android:name=".TestActivity"
            android:label="@string/title_activity_test"></activity>
        <activity
            android:name="com.kuni.activity.RecondActivity"
            android:label="@string/title_activity_recond"></activity>
        <activity
            android:name="com.kuni.activity.AEActivity"
            android:label="@string/title_activity_recond"></activity>
    </application>

</manifest>